{
  "name": "{{ projectName }}",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "limit": "eslint '**/*.{js,ts,vue}' --max-warnings 0 --fix",
    "format": "prettier '**/*.{js,ts,vue}' --write",
    "format-watch": "onchange -d 1000 '**/*.{js,ts,vue}' --prettier --config .prettierrc.json --write {{changed}}",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "commit": "cz"
  },
  "lint-staged": {
    "**/*.{js,ts,vue}": "eslint --fix --max-warnings 0"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "onchange": "^7.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  },
  "description": "{{ description }}",
  "author": "{{ author  }}"
}